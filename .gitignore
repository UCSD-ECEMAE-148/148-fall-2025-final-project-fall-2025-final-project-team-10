install/
log/

# Ignore core dump files
core
core.*

# Ignore temporary files
*.tmp
*.temp

# Ignore OS-specific files
.DS_Store
Thumbs.db

# Ignore editor files (add your editor here if you want)
.vscode/
.idea/
*.swp
*.swo

# Ignore Python cache files
